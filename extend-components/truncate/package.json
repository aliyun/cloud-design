    "access": "public"
  },
  "dependencies": {
    "@pika/react": "^16.13.1",
    "@pika/react-dom": "^16.13.1",
    "classnames": "^2.2.5",
    "lodash.truncate": "^4.4.2",
    "react-sizeme": "^2.6.7",
    "react-truncate-markup": "^5.0.0"
  },
  "devDependencies": {
    "@alicloud/console-toolkit-cli": "^1.0.8",
    "@alicloudfe/components": "^1.0.4",
    "concurrently": "^5.3.0",
    "typescript": "^3.9.7",
    "vite": "^1.0.0-rc.4",
    "vite-plugin-react-pages": "^0.8.2"
  }
